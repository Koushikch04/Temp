<div class="message__records-grid">
  <h3 class="message-heading">S.No.</h3>
  <h3></h3>
    <h3 class="message-heading">PetType</h3>
    <h3 class="message-heading">ProductType</h3>
    <h3 class="message-heading">ProductName</h3>
    <h3 class="message-heading">ProductPrice</h3>
  
    <% if(messages.length > 0) { %> <% for(let i = (currentPage-1)*limit; i < currentPage*limit && i < messages.length; i++) {
    %>
    <p class="message-pet"><%= i + 1 %></p>
    <input
      type="checkbox"
      class="message-checkbox"
      name="deleteMessage"
      value="<%= messages[i]._id %>"
    />
    <p class="message-pet"><%= messages[i].petType %></p>
    <p class="message-pet"><%= messages[i].productType %></p>
    <p class="message-pet"><%= messages[i].productDetails.Name %></p>
    <p class="message-pet"><%= messages[i].productDetails.price %></p>

    <% } %> <% } else { %>
    <h3 class="no-records">No records found</h3>
    <% } %>
  </div>